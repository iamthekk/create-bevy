{
	"name": "@white-dragon-bevy/bevy_plugin_example",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json",
		"**/default.project.json",
        "**/node_modules/**/node_modules/**",
        "**/node_modules/**/__bootstrap__/**",
        "**/node_modules/**/__examples__/**",
        "**/node_modules/**/__tests__/**",
        "!**/node_modules/**/bevy_plugin_example/**"

	],
	"tree": {
		"$className": "DataModel",
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					},
					"@flamework": {
						"$path": "node_modules/@flamework"
					},
					"@white-dragon-bevy": {
						"$path": "node_modules/@white-dragon-bevy",
						"bevy_plugin_example":{
							"$path": "out"
						}
					}
				}
			},
			"TS": {
				"$className": "Folder",
				"__examples__":{
					"$path": "out/__examples__"
				}
			},
			"bootstrap": {
				"$path": "bootstrap/shared"
			}
		},
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"bootstrap": {
				"$path": "bootstrap/server"
			}
		},
		"StarterPlayer": {
			"$className": "StarterPlayer",
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"bootstrap": {
					"$path": "bootstrap/client"
				}
			}
		},
		"TestService":{
			"$path": "node_modules/test-cloud-testez/TestService/test-cloud-testez"
		}
	}
}
